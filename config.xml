<?xml version='1.0' encoding='utf-8'?>
<widget id="pe.picnic.CarrerasTLS" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Carreras TLS</name>
    <description>
        Carreras TLS
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <preference name="Orientation" value="landscape" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon height="167" platform="ios" src="res/icon/ios/AppIcon.appiconset/Icon-App-83.5x83.5@2x.png" width="167" />
    <icon height="152" platform="ios" src="res/icon/ios/AppIcon.appiconset/Icon-App-76x76@2x.png" width="152" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <splash height="1536" src="res/screen/ios/screen-ipad-landscape-2x.png" width="2048" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <engine name="ios" spec="^4.4.0" />
    
    <plugin name="org.apache.cordova.splashscreen" spec="0.2.7" source="pgb" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
